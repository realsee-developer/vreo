{
  "name": "@realsee/vreo-monorepo",
  "version": "2.6.0",
  "private": true,
  "description": "Vreo monorepo - VR Video player for Realsee 3D spaces",
  "scripts": {
    "build": "pnpm --filter @realsee/vreo build",
    "packages": "pnpm --filter @realsee/vreo packages",
    "docs": "pnpm --filter @realsee/vreo docs",
    "dev:vreo": "pnpm --filter @realsee/vreo dev",
    "dev:test": "pnpm --filter @realsee/vreo-test dev",
    "test:build": "pnpm build && pnpm --filter @realsee/vreo-test build",
    "test:preview": "pnpm --filter @realsee/vreo-test preview"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@8.15.0"
} 